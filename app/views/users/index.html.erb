<%= render_title t('title.user.index') %>
<h1><%=t 'title.user.index' %></h1>
<% content_for :actions do %>
	<ul>
		<% if can? :write, User %>
			<%= link_to_action t('action.user.new'), new_admin_user_path %>
		<% end %>
	</ul>
<% end %>
<ul>
	<% @presenters.each do |presenter| %>
		<li><%= link_to presenter.login, presenter.path %></li>
	<% end %>
</ul>
