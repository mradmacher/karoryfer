<%= render_title @view.title %>
<h2><%= @view.title %></h2>
<p>
  <%= render partial: 'shared/show_actions', locals: { view: @view } %>
</p>
<p>
  <% if @view.embed_url %>
    <iframe width="420" height="315" src="<%= @view.embed_url %>" frameborder="0" allowfullscreen></iframe>
  <% end %>
</p>
<p>
  <%= render_text @view.body %>
</p>
<br />
<%= render partial: 'embeds/all' %>
