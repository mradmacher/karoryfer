<% content_for :sidebar do %>
  <% @artist_presenters.each do |artist_presenter| %>
    <a href="<%= artist_presenter.path %>">
      <%= image_tag(artist_presenter.image.icon.url, alt: artist_presenter.name, title: artist_presenter.name, class: 'img-rounded img-polaroid') %>
    </a>
  <% end %>
<% end %>
<div class="jumbotron">
  <h1><%=t 'label.site.name' %></h1>
  <p><%=t 'label.site.desc' %></p>
</div>
