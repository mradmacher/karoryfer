<%= render_title t 'title.video.index' %>

<%= render partial: 'sidebar' %>

<h1><%=t 'title.video.index' %></h1>

<% if @video_presenters.empty? %>
  <%=t 'label.nothing_here_yet' %>
<% end %>
<ul class="row list-unstyled">
<% @video_presenters.each do |presenter| %>
  <li class="col-sm-3 col-md-3 col-lg-3">
    <div class="thumbnail text-center"><%= render partial: 'videos/video', locals: { video: presenter } %></div>
  </li>
<% end %>
</ul>

<%= render :partial => 'events/calendar_js' %>
